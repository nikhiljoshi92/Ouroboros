digraph Flyer {

    node
        [shape=Mrecord width=1.5];

    subgraph cluster_FlyerFSM {

        label="FlyerFSM";

        //
        // States (Nodes)
        //

        "FlyerFSM::Suspended"
            [label="{Suspended}"];

        "%start"
            [label="" shape=circle style=filled fillcolor=black width=0.25];

        //
        // Transitions (Edges)
        //

        "%start" -> "FlyerFSM::Suspended"
    }

}
